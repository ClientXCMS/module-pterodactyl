<div class="col-md-12 col-xl-{{ inAdmin is not defined ? 12 : 6 }} col-sm-12">
    <div class="card">
        <div class="card-body">
            <div class="row">
                <div class="col-12">
                    {% if eggs | length > 1 %}
                        {{ field("eggname", item.eggname, "Egg pterodactyl", {options: eggs}) }}
                    {% else %}
                        <p>Selected : {{ eggs |first }}</p>
                    {% endif %}
                </div>
                {% if inFiveM %}

                    <div class="col-12">
                        {{ field("FIVEM_LICENSE", item.FIVEM_LICENSE, "Cl√© FiveM", {help: 'https://keymaster.fivem.net/'}) }}
                    </div>
                {% endif %}
                <input type="hidden" value="{{ productId }}" name="productId">
            </div>
        </div>
    </div>
</div>
